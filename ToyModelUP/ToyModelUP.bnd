node I
{
rate_up = 0.0;
rate_down = 0.0;
}

node A
{
rate_up =  I ? 1.0 : 0.0;
rate_down =  0.0;
}

node L
{
rate_up =  A ? 1.0 : 0.0;
rate_down =  0.0;
}
node R
{
rate_up =  $innerOn ? L : $outerL;
rate_down =  0.0;
}
node T1
{
rate_up = (A & (!T2)) ? 1.0 :0.0;
rate_down = 0.0;
}
node T2
{
rate_up = (R & (!A)) ? 1.0 : 0.0;
rate_down = 0.0;
}



